<Defs>

  <GeneDef ParentName="GeneJawBase">
    <defName>BS_BrokenTitanHead</defName>
    <label>urb cyborg head</label>
    <description>Carriers of this gene have partial cybernetic-looking head</description>
    <iconPath>GeneIcons/BS_TitanHead_Icon</iconPath>
    <displayOrderInCategory>97</displayOrderInCategory>
    <forcedHeadTypes>
      <!-- <li>BS_BrokenTitanHeadDefault</li> -->
      <li>BS_BrokenTitanHeadFemale</li>
      <li>BS_BrokenTitanHeadMale</li>
    </forcedHeadTypes>
    <selectionWeight>0.5</selectionWeight>
    <canGenerateInGeneSet>true</canGenerateInGeneSet>
  </GeneDef>

  <HeadTypeDef ParentName="AverageBase">
    <defName>BS_BrokenTitanHeadFemale</defName>
    <graphicPath>BS_Heads/Titan/bp_female_normal</graphicPath>
    <randomChosen>false</randomChosen>
    <gender>Female</gender>
    <selectionWeight>99999</selectionWeight>
    <requiredGenes>
      <li>BS_BrokenTitanHead</li>
    </requiredGenes>
  </HeadTypeDef>

  <HeadTypeDef ParentName="AverageBase">
    <defName>BS_BrokenTitanHeadMale</defName>
    <graphicPath>BS_Heads/Titan/bp_default_normal</graphicPath>
    <randomChosen>false</randomChosen>
    <gender>Male</gender>
    <selectionWeight>99999</selectionWeight>
    <requiredGenes>
      <li>BS_BrokenTitanHead</li>
    </requiredGenes>
  </HeadTypeDef>

  <GeneDef>
    <defName>BS_Smallhead</defName>
    <label>small head</label>
    <description>Carriers of this genes have heads that are 15% smaller than normal.</description>
    <iconPath>UI/Icons/Genes/Gene_HeadboneHuman</iconPath>
    <displayOrderInCategory>100</displayOrderInCategory>
    <symbolPack>
      <prefixSymbols>
        <li><symbol>pea-headed</symbol></li>
      </prefixSymbols>
    </symbolPack>
    <statOffsets>
      <SM_HeadSize_Cosmetic>-0.12</SM_HeadSize_Cosmetic>
    </statOffsets>
    <modExtensions>
      <li Class="BigAndSmall.PawnExtension">
        <headDrawData>
          <dataSouth>
            <offset>(0.0, 0, -0.04)</offset>
          </dataSouth>
          <dataEast>
            <offset>(0.0, 0, -0.04)</offset>
          </dataEast>
          <dataWest>
            <offset>(-0.0, 0, -0.04)</offset>
          </dataWest>
        </headDrawData>
      </li>
    </modExtensions>
  </GeneDef>
</Defs>