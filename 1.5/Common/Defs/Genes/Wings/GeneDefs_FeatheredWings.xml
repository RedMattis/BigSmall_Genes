        <colorType>Hair</colorType>
        <drawData>
          <defaultData>
            <layer>91</layer>
          </defaultData>
          <dataEast>
            <offset>(0.10, 0, -0.05)</offset>
          </dataEast>
          <dataNorth>
            <layer>91</layer>
            <offset>(0.0, 0, -0.05)</offset>
          </dataNorth>
        </drawData>
      </li>
    </renderNodeProperties>
  </GeneDef>
