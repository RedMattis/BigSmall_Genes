  <GeneDef>
    <defName>BS_StonemaskHead</defName>
    <label>stone mask</label>
    <description>Carriers of this gene have head resembling a stone mask</description>
    <iconPath>GeneIcons/Gene_StonemaskHead</iconPath>
    <displayOrderInCategory>98</displayOrderInCategory>
    <forcedHeadTypes>
      <li>BS_StonemaskHead</li>
    </forcedHeadTypes>
    <selectionWeight>0.25</selectionWeight>
    <canGenerateInGeneSet>true</canGenerateInGeneSet>
    <!-- <hairTagFilter>
      <whitelist>true</whitelist>
      <tags>
        <li>Bald</li>
      </tags>
    </hairTagFilter> -->
    <beardTagFilter>
      <whitelist>true</whitelist>
      <tags>
        <li>NoBeard</li>
      </tags>
    </beardTagFilter>
    <modExtensions>
      <li Class="BigAndSmall.PawnExtension">
				<disableFacialAnimations>true</disableFacialAnimations>
			</li>
			
		</modExtensions>
  </GeneDef>

  <HeadTypeDef ParentName="AverageBase">
