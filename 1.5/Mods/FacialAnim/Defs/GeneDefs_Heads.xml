  <GeneDef ParentName="GeneJawBase">
    <defName>BS_DevourerHead</defName>
    <label>devourer head</label>
    <description>Carriers of this gene have a menacing devourer head</description>
    <iconPath>GeneIcons/BS_DevourerHead</iconPath>
    <displayOrderInCategory>98</displayOrderInCategory>
    <selectionWeight>1</selectionWeight>
    <canGenerateInGeneSet>true</canGenerateInGeneSet>
    <hairTagFilter>
      <whitelist>true</whitelist>
      <tags>
        <li>Bald</li>
      </tags>
    </hairTagFilter>
    <beardTagFilter>
      <whitelist>true</whitelist>
      <tags>
        <li>NoBeard</li>
      </tags>
    </beardTagFilter>
    <geneClass>BetterPrerequisites.PGene</geneClass>
    <modExtensions>
      <li Class="BigAndSmall.PawnExtension">
        <disableFacialAnimations>true</disableFacialAnimations>
      </li>

    </modExtensions>
    <renderNodeProperties Inherit="False">
      <li>
        <nodeClass>PawnRenderNode_AttachmentHead</nodeClass>
        <workerClass>PawnRenderNodeWorker_FlipWhenCrawling</workerClass>
        <texPaths>
          <li>BS_Heads/Devourer/average</li>
          <li>BS_Heads/Devourer/square</li>
          <li>BS_Heads/Devourer/closed</li>
          <li>BS_Heads/Devourer/smile</li>
        </texPaths>
        <useSkinShader>false</useSkinShader>
        <colorType>Skin</colorType>
        <drawData>
          <defaultData>
            <layer>52</layer>
          </defaultData>
        </drawData>
        <parentTagDef>Head</parentTagDef>
      </li>
    </renderNodeProperties>
  </GeneDef>
  <GeneDef ParentName="GeneJawBase" Name="BS_DragonHead">
    <defName>BS_DragonHead</defName>
    <label>dragon head</label>
    <description>Carriers of this gene have an intimidating dragon-like head.</description>
    <iconPath>GeneIcons/BS_DragonHead</iconPath>
    <displayOrderInCategory>98</displayOrderInCategory>
    <selectionWeight>1</selectionWeight>
    <statFactors>
      <SM_HeadSize_Cosmetic>0.9</SM_HeadSize_Cosmetic>
    </statFactors>
    <canGenerateInGeneSet>true</canGenerateInGeneSet>
    <beardTagFilter>
      <whitelist>true</whitelist>
      <tags>
        <li>NoBeard</li>
      </tags>
    </beardTagFilter>
    <geneClass>BetterPrerequisites.PGene</geneClass>
    <modExtensions>
      <li Class="BigAndSmall.PawnExtension">
        <disableFacialAnimations>true</disableFacialAnimations>
      </li>

    </modExtensions>
    <renderNodeProperties Inherit="False">
      <li>
        <nodeClass>PawnRenderNode_AttachmentHead</nodeClass>
        <workerClass>PawnRenderNodeWorker_FlipWhenCrawling</workerClass>
        <texPaths>
          <li>BS_Heads/Dragon/average</li>
        </texPaths>
        <useSkinShader>false</useSkinShader>
        <colorType>Skin</colorType>
        <drawData>
          <defaultData>
            <layer>52</layer>
          </defaultData>
        </drawData>
        <parentTagDef>Head</parentTagDef>
      </li>
    </renderNodeProperties>
  </GeneDef>
  <GeneDef ParentName="GeneJawBase">
    <defName>BS_StonemaskHead</defName>
