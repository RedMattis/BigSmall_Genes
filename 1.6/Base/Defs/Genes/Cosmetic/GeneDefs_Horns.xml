  <GeneDef ParentName="BS_DemonHornBase">
    <defName>BS_TrollHorns</defName>
    <label>troll horns</label>
    <description>Carriers of this gene possess two branch-like horns.</description>
    <iconPath>GeneIcons/BS_Horns/Roothorns</iconPath>
    <renderNodeProperties Inherit="False">
      <li Class="BigAndSmall.PawnRenderingProps_Ultimate">
        <nodeClass>BigAndSmall.PawnRenderNode_Ultimate</nodeClass>
        <workerClass>PawnRenderNodeWorker_FlipWhenCrawling</workerClass>
        <shader>Cutout</shader>
        <conditionalGraphics>
          <colorA>
            <hairColor>true</hairColor>
            <skinColor>true</skinColor>
            <colourRange>
              <li>255,200,150</li>
              <li>200,255,150</li>
            </colourRange>
          </colorA>
          <conditionalPaths>
            <alts>
              <li>
                <chanceByAge> <points> <li>(6, 0)</li> <li>(100, 0.8)</li> <li>(200, 0.97)</li> </points> </chanceByAge>
                <randSeed>99</randSeed>
                <texturePaths>
                  <li>BS_HeadAttachments/RootOld/Roothorns2</li>
                  <li>BS_HeadAttachments/RootOld/Roothorns3</li>
                  <li>BS_HeadAttachments/RootOld/Roothorns1</li>
                </texturePaths>
              </li>
              <li>
                <chanceByAge> <points> <li>(0, 0)</li> <li>(20, 0.8)</li> <li>(100, 0.97)</li> </points> </chanceByAge>
                <randSeed>99</randSeed>
                <texturePaths>
                  <li>BS_HeadAttachments/Root/RoothornsYoung</li>
                  <li>BS_HeadAttachments/Root_Inverted/RoothornsYoung</li>
                </texturePaths>
              </li>
            </alts>
          </conditionalPaths>
          <texturePaths>
            <li>BS_Blank</li>
          </texturePaths>
        </conditionalGraphics>
        <colorType>Skin</colorType>
        <drawData>
          <defaultData> <layer>80</layer> </defaultData>
          <dataNorth> <layer>10</layer> </dataNorth>
          <dataSouth> <offset>(0, 0, -0.05)</offset> </dataSouth>
        </drawData>
        <parentTagDef>Head</parentTagDef>
      </li>
    </renderNodeProperties>
  </GeneDef>

