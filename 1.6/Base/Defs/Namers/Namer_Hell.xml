<Defs>
  <RulePackDef>
    <defName>BS_Demon_Namer</defName>
    <rulePack>
      <include>
        <li>BS_DemonFirstName</li>
        <li>BS_DemonLastName</li>
      </include>
      <rulesStrings>
        <li>r_name->[firstName] [lastName]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>BS_DemonFirstName</defName>
    <rulePack>
      <rulesStrings>
        <li>firstName->[firstNamePrefix][firstNameSuffix]</li>

        <!-- Prefixes for demon first names -->
        <li>firstNamePrefix->mal</li>
        <li>firstNamePrefix->bal</li>
        <li>firstNamePrefix->as</li>
        <li>firstNamePrefix->lil</li>
        <li>firstNamePrefix->beel</li>
        <li>firstNamePrefix->ab</li>
        <li>firstNamePrefix->demi</li>
        <li>firstNamePrefix->sam</li>
        <li>firstNamePrefix->mephi</li>
        <li>firstNamePrefix->baal</li>
        <li>firstNamePrefix->beli</li>
        <li>firstNamePrefix->abad</li>
        <li>firstNamePrefix->mol</li>
        <li>firstNamePrefix->astar</li>
        
        <!-- Suffixes for demon first names -->
        <li>firstNameSuffix->ax</li>
        <li>firstNameSuffix->ion</li>
        <li>firstNameSuffix->iel</li>
        <li>firstNameSuffix->zel</li>
        <li>firstNameSuffix->el</li>
        <li>firstNameSuffix->eth</li>
        <li>firstNameSuffix->gor</li>
        <li>firstNameSuffix->oth</li>
        <li>firstNameSuffix->os</li>
        <li>firstNameSuffix->ias</li>
        <li>firstNameSuffix->eon</li>
        <li>firstNameSuffix->don</li>
        <li>firstNameSuffix->och</li>
        <li>firstNameSuffix->tho</li>
        <li>firstNameSuffix->or</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>BS_DemonLastName</defName>
    <rulePack>
      <rulesStrings>
        <li>lastName->[lastNamePrefix][lastNameSuffix]</li>
        <li>lastName->[lastNameFull]</li>

        <!-- Prefixes for demon last names -->
        <li>lastNamePrefix->blood</li>
        <li>lastNamePrefix->dark</li>
        <li>lastNamePrefix->hell</li>
        <li>lastNamePrefix->shadow</li>
        <li>lastNamePrefix->nether</li>
        <li>lastNamePrefix->soul</li>
        <li>lastNamePrefix->chaos</li>
        <li>lastNamePrefix->lust</li>
        <li>lastNamePrefix->fear</li>
        <li>lastNamePrefix->pain</li>
        <li>lastNamePrefix->death</li>
        <li>lastNamePrefix->ero</li>
        <li>lastNamePrefix->passion</li>
        <li>lastNamePrefix->zeal</li>
        <li>lastNamePrefix->vain</li>
        <li>lastNamePrefix->void</li>
        <li>lastNamePrefix->man</li>

        <!-- Suffixes for demon last names -->
        <li>lastNameSuffix->bane</li>
        <li>lastNameSuffix->fire</li>
        <li>lastNameSuffix->fang</li>
        <li>lastNameSuffix->doom</li>
        <li>lastNameSuffix->spike</li>
        <li>lastNameSuffix->slash</li>
        <li>lastNameSuffix->wrath</li>
        <li>lastNameSuffix->gore</li>
        <li>lastNameSuffix->scorn</li>
        <li>lastNameSuffix->crush</li>
        <li>lastNameSuffix->rage</li>
        <li>lastNameSuffix->hunger</li>
        <li>lastNameSuffix->crave</li>
        <li>lastNameSuffix->love</li>
        <li>lastNameSuffix->thrist</li>
        <li>lastNameSuffix->killer</li>
        <li>lastNameSuffix->harab</li>
        <li>lastNameSuffix->seeker</li>
        <li>lastNameSuffix->indulgence</li>
        <li>lastNameSuffix->glory</li>
        <li>lastNameSuffix->heart</li>
        <li>lastNameSuffix->luster</li>
        <li>lastNameSuffix->eater</li>
        <li>lastNameSuffix->master</li>
        <li>lastNameSuffix->hollow</li>
        <li>lastNameSuffix->curse</li>

        <!-- Full one-word lastnames -->
        <li>lastNameFull->lust</li>
        <li>lastNameFull->gluttony</li>
        <li>lastNameFull->avarice</li>
        <li>lastNameFull->sloth</li>
        <li>lastNameFull->envy</li>
        <li>lastNameFull->wrath</li>
        <li>lastNameFull->pride</li>
        <li>lastNameFull->discord</li>
        <li>lastNameFull->dejection</li>
        <li>lastNameFull->kenodoxia</li>
        <li>lastNameFull->acedia</li>
        <li>lastNameFull->luxuria</li>
        <li>lastNameFull->fornicatio</li>
        <li>lastNameFull->tristitia</li>
        <li>lastNameFull->vanagloria</li>
        <li>lastNameFull->acedia</li>
        <li>lastNameFull->oblivion</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
</Defs>
