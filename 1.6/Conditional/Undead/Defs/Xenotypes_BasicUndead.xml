<Defs>
  <XenotypeDef Name="VU_Returned">
    <defName>VU_Returned</defName>
    <label>Rotted Returned</label>
    <description>A person who has come back from death for an unknown reason.</description>
    <descriptionShort>Intelligent undead</descriptionShort>
    <inheritable>true</inheritable>
    <combatPowerFactor>1.5</combatPowerFactor>
    <iconPath>Xenotypes/BS_Returned</iconPath>
    <doubleXenotypeChances>
      <Pigskin>0.02</Pigskin>
      <Impid>0.02</Impid>
      <Yttakin>0.02</Yttakin>
      <Neanderthal>0.02</Neanderthal>
      <Waster>0.02</Waster>
      <Dirtmole>0.02</Dirtmole>
      <VU_Revenant MayRequire="RedMattis.Undead">0.05</VU_Revenant>
