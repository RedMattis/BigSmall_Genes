<Defs>
  <GeneDef ParentName="GeneJawBase">
    <defName>BS_OgreHead</defName>
    <label>ogre head</label>
    <description>Carriers of this gene have a big lumpy ogre head.</description>
    <iconPath>GeneIcons/BS_OgreHead</iconPath>
    <displayOrderInCategory>97</displayOrderInCategory>

    <graphicData>
      <graphicPaths>
        <li>BS_Ogre/standard</li>
        <li>BS_Ogre/pointy</li>
        <li>BS_Ogre/square</li>
      </graphicPaths>
      <layer>PostSkin</layer>
      <colorType>Skin</colorType>				
      <useSkinShader>false</useSkinShader>
      <drawLoc>HeadTop</drawLoc>
      <drawOffset>(0, -0.00288, 0)</drawOffset>
      <drawIfFaceCovered>true</drawIfFaceCovered>						
    </graphicData>
    
    <selectionWeight>0.1</selectionWeight>
    <canGenerateInGeneSet>false</canGenerateInGeneSet>
    <modExtensions>
			<li MayRequire="OskarPotocki.VanillaFactionsExpanded.Core" Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathEndogenes>GeneIcons/BS_BackEndogene</backgroundPathEndogenes>
				<backgroundPathXenogenes>GeneIcons/BS_BackXenogene</backgroundPathXenogenes>
			</li>
		</modExtensions>
  </GeneDef>

  <GeneDef ParentName="GeneJawBase">
    <defName>BS_DevourerHead</defName>
    <label>devourer head</label>
    <description>Carriers of this gene have a menacing devourer head</description>
    <iconPath>GeneIcons/BS_DevourerHead</iconPath>
    <displayOrderInCategory>98</displayOrderInCategory>
    <graphicData>
      <graphicPaths>
        <li>BS_Heads/Devourer/average</li>
        <li>BS_Heads/Devourer/square</li>
        <li>BS_Heads/Devourer/closed</li>
        <li>BS_Heads/Devourer/smile</li>
      </graphicPaths>
      <layer>PostSkin</layer>
      <colorType>Skin</colorType>				
      <useSkinShader>false</useSkinShader>
      <drawLoc>HeadTop</drawLoc>
      <drawOffset>(0, -0.00288, 0)</drawOffset>
      <drawIfFaceCovered>true</drawIfFaceCovered>						
    </graphicData>
    <selectionWeight>1</selectionWeight>
    <canGenerateInGeneSet>true</canGenerateInGeneSet>
    <hairTagFilter>
      <whitelist>true</whitelist>
      <tags>
        <li>Bald</li>
      </tags>
    </hairTagFilter>
    <beardTagFilter>
      <whitelist>true</whitelist>
      <tags>
        <li>NoBeard</li>
      </tags>
    </beardTagFilter>
    <geneClass>BetterPrerequisites.PGene</geneClass>
    <modExtensions>
      <li Class="BetterPrerequisites.GeneExtension">
        <facialDisabler/>
      </li>
			<li MayRequire="OskarPotocki.VanillaFactionsExpanded.Core" Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathEndogenes>GeneIcons/BS_BackEndogene</backgroundPathEndogenes>
				<backgroundPathXenogenes>GeneIcons/BS_BackXenogene</backgroundPathXenogenes>
			</li>
		</modExtensions>
  </GeneDef>

